<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="399.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.projectjavawallet.controllers.setting.changeMailController">
   <children>
      <Label alignment="CENTER" prefHeight="91.0" prefWidth="402.0" text="Change Mail">
         <font>
            <Font name="Roboto Light" size="42.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="130.0" text="Current Mail">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="200.0" text="New Mail">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button layoutX="32.0" layoutY="319.0" mnemonicParsing="false" onAction="#confirmButtonClick" prefHeight="39.0" prefWidth="108.0" text="Confirm">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="257.0" layoutY="319.0" mnemonicParsing="false" onAction="#cancelButtonClick" prefHeight="39.0" prefWidth="108.0" text="Cancel">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label fx:id="currentMail" layoutX="159.0" layoutY="125.0" prefHeight="30.0" prefWidth="169.0" text="Label" />
      <TextField fx:id="newMail" layoutX="155.0" layoutY="196.0" prefHeight="30.0" prefWidth="169.0" />
   </children>
</AnchorPane>