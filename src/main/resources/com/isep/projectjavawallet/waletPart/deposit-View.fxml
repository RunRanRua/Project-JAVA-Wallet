<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="660.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.projectjavawallet.controllers.wallet.DepositController">

    <children>
        <TextField fx:id="depositAmount" layoutX="55.0" layoutY="195.0" prefHeight="51.0" prefWidth="358.0" promptText="amount">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
        <Button layoutX="372.0" layoutY="554.0" mnemonicParsing="false" onAction="#depositButtonClick" prefHeight="59.0" prefWidth="137.0" text="Deposit">
            <font>
                <Font name="System Bold" size="25.0" />
            </font>
        </Button>
        <Button layoutX="85.0" layoutY="554.0" mnemonicParsing="false" onAction="#cancelButtonClick" prefHeight="59.0" prefWidth="137.0" text="Cancel">
            <font>
                <Font name="System Bold" size="25.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="173.0" layoutY="41.0" prefHeight="45.0" prefWidth="284.0" text="Deposit amount" underline="true">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label fx:id="currency" alignment="CENTER" layoutX="441.0" layoutY="196.0" prefHeight="50.0" prefWidth="171.0" text="AAA">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>

</AnchorPane>
